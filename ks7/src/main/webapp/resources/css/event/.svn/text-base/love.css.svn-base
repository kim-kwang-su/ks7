@CHARSET "UTF-8";

div, dl, dt, dd, ul, ol, li, form, fieldset, p, blockquote {margin:0; padding:0;}
ul, ol, li {list-style:none outside;}
img {border:none}
.png24 {tmp:expression(setPng24(this));}
.event {width:712px;}
.event .todaywords {width:712px;height:25px;background:url(/ks/resources/image/event/love/bg_todaywords01.jpg);padding-top:12px;}
.event .todaywords dl dt {float:left;font-size:12px;font-weight:bold;color:#FFFFFF;padding-left:65px;}
.event .todaywords dl dd {float:left;font-size:12px;font-weight:bold;color:#f8ef8b;margin-left:15px;}
.event .todaywords dl dd img {vertical-align:middle}
.event ul.eventmaintab {width:712px;height:94px;background:url(/ks/resources/image/event/love/bg_eventtab01.jpg);padding-top:18px;}
