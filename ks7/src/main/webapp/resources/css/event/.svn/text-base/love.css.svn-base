.event ul.eventmaintab li.off {float:left;padding-left:130px;padding-top:15px;}
.event ul.eventmaintab2 {width:712px;height:94px;background:url(/ks/resources/image/event/love/bg_eventtab02.jpg);padding-top:18px;}
.event ul.eventmaintab2 li {float:left;padding-left:135px;}
.event ul.eventmaintab2 li.off {float:left;padding-left:75px;padding-top:15px;}
