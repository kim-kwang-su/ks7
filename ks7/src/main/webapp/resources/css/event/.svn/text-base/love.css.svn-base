.event .event_board .btitle2 {width:712px;height:51px;background:url(/ks/resources/image/event/love/bg_boardtitle2.jpg);}
.event .event_board .btitle p {float:left}
.event .event_board .btitle dl {float:right;padding-right:40px;margin-top:33px;}
.event .event_board .btitle dl dt {float:left}
.event .event_board .btitle dl dd {float:left}
.event .event_board .btitle dl dd img {vertical-align:middle;margin-left:3px;margin-top:-2px;}
.event .event_board .btitle dl dd select {width:90px;margin-left:3px;font-size:12px;}
.event .event_board .btitle dl dd select.w60 {width:60px;}
.event .event_board .blist {width:673px;padding:10px 0;background:url(/ks/resources/image/event/love/bg_blist.jpg) repeat-y;padding-left:39px;overflow:hidden}
