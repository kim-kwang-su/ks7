.event .event_board .blist .event_tableb table tr td {padding:5px;background:url(/ks/resources/image/event/love/bg_tableline.jpg) repeat-x bottom;text-align:left;font-size:12px;}
.event .event_board .blist .event_tableb table tr td img {vertical-align:middle;margin-left:5px;}
.event .event_board .blist .event_tableb table tr td.viewarea {padding:30px;background:none;font-size:12px;line-height:180%;color:#666666;text-align:left}
