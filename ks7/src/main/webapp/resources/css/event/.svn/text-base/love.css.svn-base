.event .event_board .blist ul li.plist {width:159px;height:147px;background:url(/ks/resources/image/event/love/bg_piclist.jpg);padding-top:16px;text-align:center;float:left;position:relative;margin-top:5px;}
.event .event_board .blist ul li.plist p.textdeco01 {position:absolute;top:0;left:20px;}
.event .event_board .blist ul li.plist dl.compic {}
.event .event_board .blist ul li.plist dl.compic dt {height:104px;} 
.event .event_board .blist ul li.plist dl.compic dd {font-size:11px;letter-spacing:-1px;color:#5a5a5a;font-weight:bold;height:16px;} 
.event .event_board .blist ul li.plist dl.compic dd span {font-weight:normal;margin-left:3px;}
.event .event_board .blist ul li.plist dl.comcountlist {clear:both;}
.event .event_board .blist ul li.plist dl.comcountlist dt {display:inline;margin-left:5px;}
.event .event_board .blist ul li.plist dl.comcountlist dd.commt {display:inline;font-size:11px;font-weight:bold;letter-spacing:-1px;margin-left:-2px;color:#3684b9;}
.event .event_board .blist ul li.plist dl.comcountlist dd.recom {display:inline;font-size:11px;font-weight:bold;letter-spacing:-1px;margin-left:-2px;color:#f97408;}
.event .event_board .bbtm {width:712px;height:55px;background:url(/ks/resources/image/event/love/bg_boardbtm.jpg);}
.event .event_board .bbtm2 {width:712px;height:55px;background:url(/ks/resources/image/event/love/bg_boardbtm2.jpg);}

.event .event_board .blist .writecom {font-size:12px;margin-top:15px;width:620px;margin-left:20px;}
.event .event_board .blist .writecom .combox {width:580px;height:112px;background:url(/ks/resources/image/event/love/bg_combox.jpg);margin-top:7px;padding:10px 10px;}
.event .event_board .blist .writecom .combox ul li {float:left;margin-left:10px;height:30px;}
.event .event_board .blist .writecom .combox .commentw {clear:both;padding-left:15px;margin-top:-10px;}
.event .event_board .blist .writecom .combox .commentw textarea {height:55px;width:440px;margin-right:10px;}
.event .event_board .blist .writecom .combox  p {padding-left:15px;margin-top:5px;font-size:11px;}
.event .event_board .blist p.grayline {height:1px;background:#d7d7d7;width:625px;margin-left:7px;margin-top:15px;}
.event .event_board .blist .btngroup {height:30px;text-align:right;margin-left:7px;width:625px;padding-top:10px;}
.event .event_board .blist .btngroup img {margin-left:3px;}

.event .event_board .blist ul.commentarea {font-size:12px;margin-top:10px;width:620px;margin-left:10px;}
.event .event_board .blist ul.commentarea li {padding:10px 5px;background:url(/ks/resources/image/event/love/bg_comlistdot.jpg) repeat-x bottom;overflow:hidden;_background:url(/ks/resources/image/event/love/bg_comlistdot.jpg) repeat-x top;}
.event .event_board .blist ul.commentarea li.linenone {_background:none;}
.event .event_board .blist ul.commentarea li dl {float:left}
.event .event_board .blist ul.commentarea li dl dt {float:left;font-weight:bold}
.event .event_board .blist ul.commentarea li dl dt img {vertical-align:middle;margin-right:3px;}
.event .event_board .blist ul.commentarea li dl dd {float:left;padding-top:1px;margin-left:5px;width:450px;line-height:140%;color:#666666;}
.event .event_board .blist ul.commentarea li p {float:right;padding-top:3px;}

 ul.pagenum { padding-top:20px; width:100%; text-align:center; clear:both;font-size:12px;height:20px;}
 ul.pagenum li { padding-left:10px; padding-right:10px; vertical-align:middle; color:#999; background:url(/ks/resources/image/event/love/bg_numbar.gif) right no-repeat; display:inline;}
 ul.pagenum li a:link,
 ul.pagenum li a:visited,
 ul.pagenum li a:active { color:#999; text-decoration:none;}
 ul.pagenum li a:hover { color:#606060; text-decoration:underline;}
 ul.pagenum li.none { padding-right:5px; color:#999; background:none;}
 ul.pagenum li.none a:link,
 ul.pagenum li.none a:visited,
 ul.pagenum li.none a:active { color:#999; text-decoration:none;}
 ul.pagenum li.none a:hover { color:#606060; text-decoration:underline;}
 ul.pagenum span.current { font-weight:bold; color:#606060;}
