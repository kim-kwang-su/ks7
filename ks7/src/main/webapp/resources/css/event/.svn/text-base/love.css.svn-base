.event .bestcommunity ul li .rank dl.comcount dd.commt {float:left;font-size:11px;font-weight:bold;color:#FFFFFF;padding-top:2px;color:#7ed2f3}
.event .bestcommunity ul li .rank dl.comcount dd.recom {float:left;font-size:11px;font-weight:bold;color:#FFFFFF;padding-top:2px;color:#f9b208}
.event .bestcommunity ul li dl.pic {width:198px;}
.event .bestcommunity ul li dl.pic dt {width:198px;height:140px;background-color:#FFFFFF;text-align:center;padding-top:3px;background:url(/ks/resources/image/event/love/bg_bestpic.jpg);}
.event .bestcommunity ul li dl.pic dd {width:198px;text-align:center;font-size:14px;font-weight:bold;color:#FFFFFF;padding-top:5px;letter-spacing:-1px}
.event .event_board .btitle {width:647px;height:60px;background:url(/ks/resources/image/event/love/bg_boardtitle.jpg);padding-left:65px;padding-top:50px;}
